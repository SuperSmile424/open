{
  "name": "@open-sauced/open-sauced",
  "description": "Open Sauced provides structured onboarding for new contributors to open source. This structure provides a way to track your next contributions by leveraging a unique dashboard built on top of the GitHub GraphQL API.",
  "version": "0.34.4",
  "private": true,
  "license": "MIT",
  "files": [
    "build"
  ],
  "bugs": "https://github.com/open-sauced/open-sauced/issues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/open-sauced/open-sauced.git"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "scripts": {
    "preinstall": "npx @open-sauced/check-engines",
    "prestart": "npx @open-sauced/check-engines",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "clean": "rimraf src/tests/__snapshots__/",
    "test": "npm run clean && node scripts/test.js --env=jsdom --updateSnapshot",
    "storybook": "start-storybook -p 6006 -s ./.storybook",
    "build-storybook": "build-storybook -s ./.storybook",
    "push": "cz",
    "lint": "eslint . --report-unused-disable-directives",
    "format": "npm run lint -- --fix",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@babel/runtime": "^7.15.4",
    "@open-sauced/check-engines": "^1.0.0",
    "@open-sauced/semantic-release-conventional-config": "^1.4.0",
    "@storybook/addon-essentials": "^6.3.10",
    "@storybook/addon-links": "^6.3.10",
    "@storybook/addon-storysource": "^6.3.10",
    "@storybook/api": "^6.3.10",
    "@storybook/react": "^6.3.10",
    "@testing-library/dom": "^8.7.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-jest": "^27.2.5",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "case-sensitive-paths-webpack-plugin": "^2.4.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^4.6.1",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "6.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "file-loader": "^6.2.0",
    "jest": "^27.2.5",
    "jest-axe": "^5.0.1",
    "jest-localstorage-mock": "^2.4.18",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.6.2",
    "object-assign": "^4.1.1",
    "postcss-loader": "^4.3.0",
    "prettier": "^2.4.1",
    "promise": "^8.1.0",
    "react-dev-utils": "^11.0.4",
    "react-is": "^17.0.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.4.7",
    "storybook-dark-mode": "^1.0.8",
    "style-loader": "^2.0.0",
    "url-loader": "4.1.1",
    "webpack": "^4.41.5",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "@apollo/client": "^3.4.16",
    "@primer/octicons-react": "^16.0.0",
    "@remirror/extension-bold": "^1.0.6",
    "@remirror/extension-code": "^1.0.6",
    "@remirror/extension-heading": "^1.0.6",
    "@remirror/extension-italic": "^1.0.6",
    "@remirror/extension-list": "^1.1.1",
    "@remirror/extension-markdown": "^1.0.6",
    "@remirror/extension-tables": "^1.0.7",
    "@remirror/react": "^1.0.11",
    "@remirror/styles": "^1.1.2",
    "contrast": "^1.0.1",
    "dayjs": "^1.10.7",
    "graphql": "^15.6.1",
    "humanize-duration": "^3.27.0",
    "onegraph-apollo-client": "^3.1.2",
    "onegraph-auth": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-keys": "^2.7.1",
    "react-loading-skeleton": "^2.2.0",
    "react-markdown": "^6.0.3",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "styled-components": "^5.3.1",
    "swr": "^1.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "globals": {
      "window": true
    },
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/setupTestFramework.js",
      "jest-localstorage-mock"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js|jsx)$"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
